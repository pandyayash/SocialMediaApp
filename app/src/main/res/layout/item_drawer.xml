<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
  android:id="@+id/ll_main"
  android:layout_width="match_parent"
  android:layout_height="wrap_content"
  android:layout_gravity="center"
  android:gravity="center|start"
  android:orientation="vertical">
  <LinearLayout
    android:id="@+id/ll_contain"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    android:layout_marginTop="@dimen/llTabMain_margin_tb"
    android:layout_marginBottom="@dimen/llTabMain_margin_tb"
    android:layout_marginStart="@dimen/icon_back_left_margin"
    android:layout_marginEnd="@dimen/icon_back_left_margin"
    android:layout_marginLeft="@dimen/icon_back_left_margin"
    android:layout_marginRight="@dimen/icon_back_left_margin"
    android:orientation="horizontal">

    <technource.autoreum.CustomViews.Widgets.Poppins_Regular_Textview
      android:id="@+id/tv_title"
      android:layout_width="match_parent"
      android:layout_height="wrap_content"
      android:layout_weight="1"
      android:layout_marginStart="@dimen/cb_keepSignIn_margin_t"
      android:layout_marginEnd="@dimen/icon_back_left_margin"
      android:layout_marginLeft="@dimen/cb_keepSignIn_margin_t"
      android:layout_marginRight="@dimen/icon_back_left_margin"
      android:layout_centerInParent="true"
      android:padding="@dimen/edittextpadding"
      android:gravity="right"
      android:text="@string/app_name"
      android:textAllCaps="true"
      android:textColor="@color/black"
      android:textSize="@dimen/txt_size_3" />

  </LinearLayout>


</LinearLayout>
