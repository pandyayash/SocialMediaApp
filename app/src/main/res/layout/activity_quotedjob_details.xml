                android:layout_height="wrap_content"
                android:background="@color/bg_gray"
                android:layout_margin="@dimen/SignIn_margin"
                android:paddingTop="@dimen/icon_back_left_margin"
                android:paddingBottom="@dimen/icon_back_left_margin">

                <aayushiprojects.greasecrowd.CustomViews.Widgets.Poppins_Medium_Textview
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:text="@string/car_detail"
                    android:textColor="@color/colorAccent"
                    android:gravity="center_horizontal"/>



            </LinearLayout>
            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginLeft="@dimen/SignIn_margin"
                android:layout_marginRight="@dimen/SignIn_margin"
                android:orientation="horizontal">
                <aayushiprojects.greasecrowd.CustomViews.Widgets.Poppins_Regular_Textview
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:text="Make :"
                    android:gravity="left"
                    android:layout_weight="1"
                    android:textColor="@color/black"
                    android:layout_marginLeft="@dimen/SignIn_margin"/>
                <aayushiprojects.greasecrowd.CustomViews.Widgets.Poppins_Regular_Textview
                    android:id="@+id/txtMake"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:text="@string/app_name"
                    android:gravity="start"
                    android:layout_weight="1"
                    android:paddingLeft="@dimen/icon_back_left_margin"
                    android:background="@drawable/blue_textview_border"
                    android:layout_marginLeft="@dimen/SignIn_margin"/>


            </LinearLayout>
            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginTop="@dimen/icon_back_left_margin"
                android:layout_marginLeft="@dimen/SignIn_margin"
                android:layout_marginRight="@dimen/SignIn_margin"
                android:orientation="horizontal">
                <aayushiprojects.greasecrowd.CustomViews.Widgets.Poppins_Regular_Textview
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:text="Model :"
                    android:gravity="left"
                    android:layout_weight="1"
                    android:textColor="@color/black"
                    android:layout_marginLeft="@dimen/SignIn_margin"/>
                <aayushiprojects.greasecrowd.CustomViews.Widgets.Poppins_Regular_Textview
                    android:id="@+id/txtModel"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:text="@string/app_name"
                    android:gravity="start"
                    android:layout_weight="1"
                    android:paddingLeft="@dimen/icon_back_left_margin"
                    android:background="@drawable/blue_textview_border"
                    android:layout_marginLeft="@dimen/SignIn_margin"/>




            </LinearLayout>
            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginTop="@dimen/icon_back_left_margin"
                android:layout_marginLeft="@dimen/SignIn_margin"
                android:layout_marginRight="@dimen/SignIn_margin"
                android:orientation="horizontal">
                <aayushiprojects.greasecrowd.CustomViews.Widgets.Poppins_Regular_Textview
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:text="Badge/Variant :"
                    android:gravity="left"
                    android:layout_weight="1"
                    android:textColor="@color/black"
                    android:layout_marginLeft="@dimen/SignIn_margin"/>
                <aayushiprojects.greasecrowd.CustomViews.Widgets.Poppins_Regular_Textview
                    android:id="@+id/txtBadge"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:text="@string/app_name"
                    android:gravity="start"
                    android:layout_weight="1"
                    android:paddingLeft="@dimen/icon_back_left_margin"
                    android:background="@drawable/blue_textview_border"
                    android:layout_marginLeft="@dimen/SignIn_margin"/>




            </LinearLayout>
            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginTop="@dimen/icon_back_left_margin"
                android:layout_marginLeft="@dimen/SignIn_margin"
                android:layout_marginRight="@dimen/SignIn_margin"
                android:orientation="horizontal">
                <aayushiprojects.greasecrowd.CustomViews.Widgets.Poppins_Regular_Textview
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:text="Registartion Number :"
                    android:gravity="left"
                    android:layout_weight="1"
                    android:textColor="@color/black"
                    android:layout_marginLeft="@dimen/SignIn_margin"/>
                <aayushiprojects.greasecrowd.CustomViews.Widgets.Poppins_Regular_Textview
                    android:id="@+id/txtRegNumber"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:text="@string/app_name"
                    android:gravity="start"
                    android:layout_weight="1"
                    android:paddingLeft="@dimen/icon_back_left_margin"
                    android:background="@drawable/blue_textview_border"
                    android:layout_marginLeft="@dimen/SignIn_margin"/>




            </LinearLayout>
            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginTop="@dimen/icon_back_left_margin"
                android:layout_marginLeft="@dimen/SignIn_margin"
                android:layout_marginRight="@dimen/SignIn_margin"
                android:orientation="horizontal">
                <aayushiprojects.greasecrowd.CustomViews.Widgets.Poppins_Regular_Textview
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:text="Transmission :"
                    android:gravity="left"
                    android:layout_weight="1"
                    android:textColor="@color/black"
                    android:layout_marginLeft="@dimen/SignIn_margin"/>
                <aayushiprojects.greasecrowd.CustomViews.Widgets.Poppins_Regular_Textview
                    android:id="@+id/txtTransmission"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:text="@string/app_name"
                    android:gravity="start"
                    android:layout_weight="1"
                    android:paddingLeft="@dimen/icon_back_left_margin"
                    android:background="@drawable/blue_textview_border"
                    android:layout_marginLeft="@dimen/SignIn_margin"/>




            </LinearLayout>
            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginTop="@dimen/icon_back_left_margin"
                android:layout_marginLeft="@dimen/SignIn_margin"
                android:layout_marginRight="@dimen/SignIn_margin"
                android:orientation="horizontal">
                <aayushiprojects.greasecrowd.CustomViews.Widgets.Poppins_Regular_Textview
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:text="Year :"
                    android:gravity="left"
                    android:layout_weight="1"
                    android:textColor="@color/black"
                    android:layout_marginLeft="@dimen/SignIn_margin"/>
                <aayushiprojects.greasecrowd.CustomViews.Widgets.Poppins_Regular_Textview
                    android:id="@+id/txtYear"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:text="@string/app_name"
                    android:gravity="start"
                    android:layout_weight="1"
                    android:paddingLeft="@dimen/icon_back_left_margin"
                    android:background="@drawable/blue_textview_border"
                    android:layout_marginLeft="@dimen/SignIn_margin"/>




            </LinearLayout>
            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:background="@color/bg_gray"
                android:layout_margin="@dimen/SignIn_margin"
                android:paddingTop="@dimen/icon_back_left_margin"
                android:paddingBottom="@dimen/icon_back_left_margin">

                <aayushiprojects.greasecrowd.CustomViews.Widgets.Poppins_Medium_Textview
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:text="@string/photos_videos"
                    android:textColor="@color/colorAccent"
                    android:gravity="center_horizontal"/>



            </LinearLayout>
            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:layout_marginTop="@dimen/icon_back_left_margin"
                android:layout_marginLeft="@dimen/SignIn_margin"
                android:layout_marginRight="@dimen/SignIn_margin"
                android:orientation="horizontal">

                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:orientation="vertical"
                    android:layout_weight="1">



                    <android.support.v4.view.ViewPager
                        android:id="@+id/viewpager"
                        android:layout_width="match_parent"
                        android:layout_height="100dp"/>

                    <me.relex.circleindicator.CircleIndicator
                        android:id="@+id/indicator"
                        android:layout_width="fill_parent"
                        android:layout_height="40dp"
                        app:ci_drawable="@drawable/selecteditem_dot"
                        app:ci_drawable_unselected="@drawable/nonselecteditem_dot"
                        />




                </LinearLayout>
                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:orientation="vertical"
                    android:layout_weight="1">
                    <android.support.v4.view.ViewPager
                        android:id="@+id/viewpager1"
                        android:layout_width="match_parent"
                        android:layout_height="100dp"/>

                    <me.relex.circleindicator.CircleIndicator
                        android:id="@+id/indicator1"
                        android:layout_width="fill_parent"
                        android:layout_height="40dp"
                        app:ci_drawable="@drawable/selecteditem_dot"
                        app:ci_drawable_unselected="@drawable/nonselecteditem_dot"
                        />

                </LinearLayout>




            </LinearLayout>
            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:orientation="vertical"
                android:layout_margin="@dimen/SignIn_margin">

                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:orientation="horizontal"
                    android:weightSum="2"
                    android:layout_weight="1">
                    <aayushiprojects.greasecrowd.CustomViews.Widgets.Poppins_Regular_Textview
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:text="Drop off :"
                        android:gravity="left"
                        android:layout_weight="1"
                        android:textColor="@color/black"
                        android:layout_marginLeft="@dimen/SignIn_margin"/>
                    <aayushiprojects.greasecrowd.CustomViews.Widgets.Poppins_Regular_Textview
                        android:id="@+id/txtDropOffDate"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:text="15-Dec-2017 1:10"
                        android:gravity="start"
                        android:layout_weight="1"
                        android:paddingLeft="@dimen/icon_back_left_margin"
                        android:background="@drawable/blue_textview_border"
                        android:layout_marginLeft="@dimen/icon_back_left_margin"/>

                </LinearLayout>
                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:orientation="horizontal"
                    android:layout_marginTop="@dimen/icon_back_left_margin"
                    android:weightSum="2"
                    android:layout_weight="1">
                    <aayushiprojects.greasecrowd.CustomViews.Widgets.Poppins_Regular_Textview
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:text="Pick Up :"
                        android:gravity="left"
                        android:layout_weight="1"
                        android:textColor="@color/black"
                        android:layout_marginLeft="@dimen/SignIn_margin"/>
                    <aayushiprojects.greasecrowd.CustomViews.Widgets.Poppins_Regular_Textview
                        android:id="@+id/txtPickupDate"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:text="15-Dec-2017 1:10"
                        android:gravity="start"
                        android:layout_weight="1"
                        android:paddingLeft="@dimen/icon_back_left_margin"
                        android:background="@drawable/blue_textview_border"
                        android:layout_marginLeft="@dimen/icon_back_left_margin"/>

                </LinearLayout>
                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:orientation="horizontal"
                    android:layout_marginTop="@dimen/icon_back_left_margin"
                    android:weightSum="2"
                    android:layout_weight="1">
                    <aayushiprojects.greasecrowd.CustomViews.Widgets.Poppins_Regular_Textview
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:text="Time Flexibility :"
                        android:gravity="left"
                        android:layout_weight="1"
                        android:textColor="@color/black"
                        android:layout_marginLeft="@dimen/SignIn_margin"/>
                    <aayushiprojects.greasecrowd.CustomViews.Widgets.Poppins_Regular_Textview
                        android:id="@+id/txtFlexibility"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:text="Very Flexibal"
                        android:gravity="start"
                        android:layout_weight="1"
                        android:paddingLeft="@dimen/icon_back_left_margin"
                        android:background="@drawable/blue_textview_border"
                        android:layout_marginLeft="@dimen/icon_back_left_margin"/>

                </LinearLayout>


            </LinearLayout>
            <LinearLayout
                android:id="@+id/ll_emergency"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:background="@color/colorAccent"
                android:layout_marginTop="@dimen/SignIn_margin"
                android:layout_marginLeft="@dimen/SignIn_margin"
                android:layout_marginRight="@dimen/SignIn_margin"
                android:paddingTop="@dimen/icon_back_left_margin"
                android:paddingBottom="@dimen/icon_back_left_margin"
                android:visibility="gone">


                <aayushiprojects.greasecrowd.CustomViews.Widgets.Poppins_Medium_Textview
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:text="@string/str_Emergency"
                    android:textColor="@color/white"
                    android:gravity="center_horizontal"/>

            </LinearLayout>
            <LinearLayout
                android:id="@+id/ll_insurance_claim"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:background="@color/colorAccent"
                android:layout_marginTop="@dimen/SignIn_margin"
                android:layout_marginLeft="@dimen/SignIn_margin"
                android:layout_marginRight="@dimen/SignIn_margin"
                android:paddingTop="@dimen/icon_back_left_margin"
                android:paddingBottom="@dimen/icon_back_left_margin"
