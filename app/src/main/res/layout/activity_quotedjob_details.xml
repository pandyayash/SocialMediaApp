<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:id="@+id/activity_mycrowdgarage"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical"
    android:background="@color/white"
    tools:context="aayushiprojects.greasecrowd.activities.MycrowdgarageActivity">

    <include layout="@layout/app_header" />

    <ScrollView
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_weight="1">
        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:orientation="vertical"
            android:background="@color/white">

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_margin="@dimen/SignIn_margin">
                <android.support.v7.widget.RecyclerView
                    android:id="@+id/recyclerView"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content">

                </android.support.v7.widget.RecyclerView>

            </LinearLayout>

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:background="@color/bg_gray"
                android:layout_margin="@dimen/SignIn_margin"
                android:paddingTop="@dimen/icon_back_left_margin"
                android:paddingBottom="@dimen/icon_back_left_margin">

                <aayushiprojects.greasecrowd.CustomViews.Widgets.Poppins_Medium_Textview
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:text="@string/job_detail"
                    android:textColor="@color/colorAccent"
                    android:gravity="center_horizontal"/>



            </LinearLayout>
            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginLeft="@dimen/SignIn_margin"
                android:layout_marginRight="@dimen/SignIn_margin"
                android:orientation="horizontal">
                <aayushiprojects.greasecrowd.CustomViews.Widgets.Poppins_Regular_Textview
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:text="Job Title :"
                    android:gravity="left"
                    android:layout_weight="1"
                    android:textColor="@color/black"
                    android:layout_marginLeft="@dimen/SignIn_margin"/>
                <aayushiprojects.greasecrowd.CustomViews.Widgets.Poppins_Regular_Textview
                    android:id="@+id/txtJobTitle"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:text="@string/app_name"
                    android:gravity="start"
                    android:layout_weight="1"
                    android:paddingLeft="@dimen/icon_back_left_margin"
                    android:background="@drawable/blue_textview_border"
                    android:layout_marginLeft="@dimen/SignIn_margin"/>


            </LinearLayout>
            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginLeft="@dimen/SignIn_margin"
                android:layout_marginRight="@dimen/SignIn_margin"
                android:orientation="horizontal">
                <aayushiprojects.greasecrowd.CustomViews.Widgets.Poppins_Regular_Textview
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:text="Job# :"
                    android:gravity="left"
                    android:layout_weight="1"
                    android:textColor="@color/black"
                    android:layout_marginLeft="@dimen/SignIn_margin"/>
                <aayushiprojects.greasecrowd.CustomViews.Widgets.Poppins_Regular_Textview
                    android:id="@+id/txtJobNumber"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:text="@string/app_name"
                    android:gravity="start"
                    android:layout_weight="1"
                    android:paddingLeft="@dimen/icon_back_left_margin"
                    android:background="@drawable/blue_textview_border"
                    android:layout_marginLeft="@dimen/SignIn_margin"/>


            </LinearLayout>
            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginLeft="@dimen/SignIn_margin"
                android:layout_marginRight="@dimen/SignIn_margin"
                android:orientation="horizontal">
                <aayushiprojects.greasecrowd.CustomViews.Widgets.Poppins_Regular_Textview
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:text="Category :"
                    android:gravity="left"
                    android:layout_weight="1"
                    android:textColor="@color/black"
                    android:layout_marginLeft="@dimen/SignIn_margin"/>
                <aayushiprojects.greasecrowd.CustomViews.Widgets.Poppins_Regular_Textview
                    android:id="@+id/txtCategory"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:text="@string/app_name"
                    android:gravity="start"
                    android:layout_weight="1"
                    android:paddingLeft="@dimen/icon_back_left_margin"
                    android:background="@drawable/blue_textview_border"
                    android:layout_marginLeft="@dimen/SignIn_margin"/>


            </LinearLayout>
            <LinearLayout
